            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell (Extract petTypeId)" enabled="true">
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import com.eclipsesource.json.*;

String responseAsString = prev.getResponseDataAsString();
//log.info(&quot;*** responseAsString: &quot; + responseAsString);
JsonArray petTypes = Json.parse(responseAsString);

for (JsonValue petType : petTypes) {
    JsonObject extractedPetType = petType.asObject();
    String extName = extractedPetType.get(&quot;name&quot;).asString();
    String extId = extractedPetType.get(&quot;id&quot;).asLong().toString();

    if (extName.equals(vars.get(&quot;petTypeName&quot;))){
      log.info(&quot;*** PET TYPE | Found by: (&quot; + extName + &quot;), with ID: &quot; + extId);
      vars.put(&quot;petTypeId&quot;, extId); 
      break;
    }
}

log.info(&quot;*** PET TYPE | variable &apos;petTypeId&apos; set to: &quot; + vars.get(&quot;petTypeId&quot;));</stringProp>
              <stringProp name="TestPlan.comments">Setting dynamically the petTypeId to be used for the relevant requests</stringProp>
